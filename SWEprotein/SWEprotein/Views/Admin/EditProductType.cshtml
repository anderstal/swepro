@model SWEprotein.Models.tbProductType

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Editera produkttyp</title>   <link href="~/Content/bootstrap.css" rel="stylesheet" />
</head>
<body>
    <script src="~/Scripts/jquery-1.9.1.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    
    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true)
    
        <fieldset>
            <legend>Editera produkttyp</legend>
    
            @Html.HiddenFor(model => model.iID)
    
            <div class="editor-label">
            Namn
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.sName)
                @Html.ValidationMessageFor(model => model.sName)
            </div>
     <div class="editor-label">
            Kategori
            </div>
            <div class="editor-field">
                @Html.DropDownList("iProductCategory")
                @Html.ValidationMessageFor(model => model.iProductCategory)
            </div>
            <p>
                <input type="submit" value="Spara" />
            </p>
        </fieldset>
    }
    
    <div>
         @Html.ActionLink("Adminsidan", "Index") |    @Html.ActionLink("Till listan", "ProductType")
    </div>
</body>
</html>
