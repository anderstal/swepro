@model SWEprotein.Models.tbOrder

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>EditOrder</title>   <link href="~/Content/bootstrap.css" rel="stylesheet" />
</head>
<body>
    <script src="~/Scripts/jquery-1.9.1.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    
    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true)
    
        <fieldset>
            <legend>tbOrder</legend>
    
            @Html.HiddenFor(model => model.iID)
    
            <div class="editor-label">
              AnvändarID
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.UserID)
                @Html.ValidationMessageFor(model => model.UserID)
            </div>
    
            <div class="editor-label">
              Status
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.iStatus)
                @Html.ValidationMessageFor(model => model.iStatus)
            </div>
    
            <div class="editor-label">
              Summa
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.iSum)
                @Html.ValidationMessageFor(model => model.iSum)
            </div>
    
            <div class="editor-label">
               OrderDatum
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.dtOrderDate)
                @Html.ValidationMessageFor(model => model.dtOrderDate)
            </div>
    
            <p>
                <input type="submit" value="Spara" />
            </p>
        </fieldset>
    }
    
    <div>
        @Html.ActionLink("Adminsidan", "Index") |   
         @Html.ActionLink("Till listan", "Orders")
    </div>
</body>
</html>
