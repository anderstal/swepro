@model SWEprotein.Models.UserInfo

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Redigera användare</title>
</head>
<body>
    <script src="~/Scripts/jquery-1.9.1.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    
    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true)
    
        <fieldset>
            <legend>Redigera användare</legend>
    
            @Html.HiddenFor(model => model.iID)
    
          
    
            <div class="editor-label">
              Adress
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.Adress)
                @Html.ValidationMessageFor(model => model.Adress)
            </div>
    
            <div class="editor-label">
              Postnummer
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.PostalNumber)
                @Html.ValidationMessageFor(model => model.PostalNumber)
            </div>
    
            <div class="editor-label">
             Stad
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.sCity)
                @Html.ValidationMessageFor(model => model.sCity)
            </div>
    
            <div class="editor-label">
           Email
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.sEmail)
                @Html.ValidationMessageFor(model => model.sEmail)
            </div>
    
            <div class="editor-label">
             Telefonnummer
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.sTelephone)
                @Html.ValidationMessageFor(model => model.sTelephone)
            </div>
    
       
    
            <p>
                <input type="submit" value="Spara" />
            </p>
        </fieldset>
    }
    
    <div>
          @Html.ActionLink("Tillbaka", "Index", "User")
    </div>
</body>
</html>
