                                                                       <ul class="dropdown-menu">
                                                                           <li>@Html.ActionLink("Byxor", "Sortiment", "Product", new {id = 4}, null)</li>
                                                                           <li>@Html.ActionLink("Tröjor", "Sortiment", "Product", new {id = 14}, null)</li>
                                                                       </ul>
                                                                   </li>
                                                                 
                                                            <li>@Html.ActionLink("Kostschema", "KostScheman", "Info")</li>
                         <li class="dropdown">
