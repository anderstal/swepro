@using SWEprotein.Models
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
  
  
    
    @Styles.Render("~/Content/bootstrapcss") 
  
    <style type="text/css">
        .margintop {
             margin-top: 30px;
            margin-bottom: 0;
         }

        .borderbody {
            background:white;
             
        }
         .navbar-collapse, .navbar-default .navbar-form {
border-color: #e7e7e7;
             height: 30px;
}
        .menu {
            background-color: white;
        
          font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
        }         
    </style>
</head>
    <body style="background-color:darkgrey">
        
        
          
        <div class="container" style="background:black; background-color:black;">
            <div class="row clearfix" id="divnavbartop">
	
                <div class="col-md-12">
	      
                    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1"> <span class="sr-only">Toggle navigation</span><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span></button> <a class="navbar-brand" href="/Product/Index"><img src="/Images/Sitegraf/logoSweprot.png" style="height:60px;" /></a>
                        </div>
				
                        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                            <ul class="nav navbar-nav">
                                <li>
                                    <a href="#"><img src="/images/Sitegraf/facebook-icon.png" style="height:30px"/></a>
                                </li>
                                <li>
                                    <a href="#"><img src="/images/Sitegraf/email-icon.jpg" style="height:30px"/></a>
                                </li>
                                <li><a href="/Account/Login" style="padding-top:15px;"><span class="glyphicon glyphicon-user"></span>Registrera/Logga in</a></li>
                                <li class="dropdown">
							
                            </ul>
                            
                          
                            <form class="navbar-form navbar-left" role="search">
                                <div class="form-group">
                                  @using (Html.BeginForm("ProductSearch", "Shared", FormMethod.Post, new { @class = "form-control", role = "search"}))
                                  {
                                      @Html.TextBox("searchString")
                                      <input type="submit" value="Sök"/>
                                  }
                                  
                               </div>
                            </form>
                        
                   
                                
                            <ul class="nav navbar-nav navbar-right">
                                <li>
                                    <a href="/Orders/Kundkorg"><img src="/images/Sitegraf/cart.png" style="height:30px" />Kundkorg<span class="badge">@ViewBag.cartCount</span></a>
                                </li>
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" style="padding-top:15px; margin-right:50px;"><span class="glyphicon glyphicon-info-sign"></span>  Info<strong class="caret"></strong></a>
                                    <ul class="dropdown-menu">
                                        <li>
                                            <a href="/Info/Info">Om företet</a>
                                        </li>
                                        <li>
                                            <a href="/Info/Info">FAQ</a>
                                        </li>
                                        <li>
                                            <a href="/Info/Info">Partners</a>
                                        </li>
                                        <li class="divider">
                                        </li>
                                        <li>
                                            <a href="/Info/Info">Support</a>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
				
                    </nav>
                </div>
              
            </div>
	
            <div class="row clearfix margintop" id="divlogo">
                <div class="col-md-1" style="padding:0;">
                    <a href="/Product/Index"> <img src="/Images/Sitegraf/logoSweprot.png" style="height:150px; width:300px;"/></a></div>
                <div class="col-md-2"></div>
                <div class="col-md-5"><</div>
                <div class="col-md-4"></div>
            </div>
            <div class="row clearfix" id="divmenu">
              
                <div class="col-md-12 menu"><ul class="nav nav-tabs nav-justified menu">
                                                                   <li>@Html.ActionLink("Topplistan", "Topplistan", "Product")</li>
                                                                   <li class="dropdown">
