                                                                           <li>@Html.ActionLink("GangsterLeo", "KostScheman", "Info", new {id = 14}, null)</li>
                                                                             <li>@Html.ActionLink("KarateÖster", "KostScheman", "Info", new {id = 14}, null)</li>
                                                                       </ul>
                                                                   </li>
                                                                   <li>@Html.ActionLink("Kontakta oss", "KontaktaOss", "Info")</li>
                            
                

                                                               </ul></div>
              
            </div>
        <div class="row clearfix" id="divsite" style="margin-top:10px;">
            <div class="col-md-2" id="divleftbar"><div class="caption">
                                                      <div id="carousel-example-generic" class="carousel" data-ride="carousel">
                                                          <!-- Indicators -->
                                                          <ol class="carousel-indicators">
                                                              <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
                                                              <li data-target="#carousel-example-generic" data-slide-to="1"></li>
                                                              <li data-target="#carousel-example-generic" data-slide-to="2"></li>
                                                          </ol>

                                                          <!-- Wrapper for slides -->
                                                          <div class="carousel-inner">
                                                              <div class="item active">
                                                                  <a href="/Info/Erbjudanden"><img src="/Images/Sitegraf/gymhusetrea.jpg" alt=""></a>
                                                                  <div class="carousel-caption">

                                                                  </div>
                                                              </div>
                                                              <div class="item">
                                                                  <a href="/Info/Erbjudanden"><img src="/Images/Sitegraf/gymhusetrea2.jpg" alt=""></a>
                                                                  <div class="carousel-caption">
  
                                                                  </div>
                                                              </div>
                                                              <div class="item">
                                                                  <a href="/Info/Erbjudanden">  <img src="/Images/Sitegraf/gymhusetrea3.jpg" alt=""></a>
